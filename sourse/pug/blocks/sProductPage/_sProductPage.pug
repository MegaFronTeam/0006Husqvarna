mixin sProductPage()
	// start sProductPage
	+b.sProductPage.section#sProductPage&attributes(attributes)
		.container
			.row
				.col-lg-8
					+productHeader()(class="d-block d-lg-none")
					+fotoGalery()(class="")
					+buyBlock04()(class="d-block d-lg-none block-to-calculate-top-js")
					+buyBlock04()(class="d-flex d-lg-none fixed-buy-block")
					+productInfo()(class="d-block d-lg-none")
					+actionsSlider04()(class="d-block d-lg-none")
					+productDesription()(class="")
				.col-lg-4.d-none.d-lg-block
					+productHeader()(class="")(class="")
					+productInfo()(class="")
					+buyBlock04()(class="")
					+actionsSlider04()(class="")
		.you-may-like-slider-cont
			+headline('ПОХОЖИЕ ТОВАРЫ')
			+sCatalogHit(false)
	// end sProductPage

