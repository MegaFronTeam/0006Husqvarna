							li
								a(href="#") Бензорезы
							li
								a(href="#") Рельсорезы
							li
								a(href="#") Виброрейки
							li
								a(href="#") Вибраторы
							li
								a(href="#") Затирочные машины
							li
								a(href="#") Оборудование  для алмазного бурения
							li
								a(href="#") Плиткорезы
							li
								a(href="#") Камнерезные станки
							li
								a(href="#") Нарезчик швов
							li
								a(href="#") Шлифовальные машины
							li
								a(href="#") Стенорезные машины
							li
								a(href="#") Канатные машины
							li
								a(href="#") Строительные пылесосы
							li
								a(href="#") Принадлежности
						+listFrame()
						+listFrame()
						+listFrame()
					+b.filter-bl
						+e.pop-up-controll-panel.d-flex.d-lg-none
							+e.back-btn
								+icon('angle-left')
								span Назад
							+e.discard-filters-btn
								+icon('cross')
								span сбросить фильтры
						+form-wrap()
							.check-box-filter-group
