mixin filterBtnsBar()
	+b.filter-bar-btns.d-flex.d-lg-none
		+e.filter-btn
			+icon('filter-bar-icon')
			span ФИЛЬТР парамтеров
			.filter-number 3
		+e.cross-btn.d-none.d-sm-flex
			+icon('cross')
mixin catalogGoodsTableItems()
	+b.table-items-box(data-tab-for='table-box' data-tab-content='catalog-goods-display')
		+item-prod-table-sample('category-6-png.png')(class="content-visiable")
		+item-prod-table-sample('category-6-png.png', 'star-bestseller', 'Бестселлер')(class="bestseller-item ")
		+item-prod-table-sample('category-6-png.png', 'star-new-item', 'Новинка')(class="new-item")
		+item-prod-table-sample('category-6-png.png', 'action-item','Акция')(class="action-item")


mixin item-prod-table-sample(i, iconSrc, spanTxt)
	+b.table-sample-item&attributes(attributes)
		+e.header
			if iconSrc
				+b.img-block
					+icon(iconSrc)
					+e.img-block-content
						span= spanTxt
						img.lazy(src="#" data-src=`img/@2x/${i}`, alt="some-img")
			else
				+b.img-block
					+e.img-block-content
						img.lazy(src="#" data-src=`img/@2x/${i}`, alt="some-img")
			+b.char-block
				+e.headline Триммер бензиновый Husqvarna 122C
				+e.char-list.d-none.d-xl-block
					+e.char-item
						span HUS Usage
						span Occasional use
					+e.char-item
						span Рабочий объем двигателя
						span 21.7 см³
					+e.char-item
						span Рабочий объем цилиндра
						span 1.3 куб. дюйм
					+e.char-item
						span Номинальная мощность
						span 0.6 кВт
				+e.show-more-btn.toggle-chars-goods-table-js
					span Характеристики
					+icon('chevron-down')
			+b.buy-block
				+e.content
					+e.labels
						+e.label-link.active(type="button")
							+icon("compare")
						+e.label-link(type="button")
							+icon("like-alt")
					+e.old-price 15 990 ₽
					+e.new-price 11 490 ₽
					+e.buy-btn
						a(href="#") Купить
		+b.description-of-table-items
			+e.row-item
				span Номинальная потребляемая мощность
				span 1100 Вт
			+e.row-item
				span Напряжение сети
				span 220 - 240 В
			+e.row-item
				span Макс. производительность
				span 4000 л/ч
			+e.row-item
				span Максимальная высота всасывания
				span Максимальная высота всасывания	8 м
			+e.row-item
				span Макс. высота подачи
				span Макс. высота подачи	46 м
			+e.row-item
				span Макс. давление
				span Макс. давление	4,6 бар

mixin catalogGoodsSquareItems()
	+b.square-items-box.active(data-tab-for='square-box' data-tab-content='catalog-goods-display')
		.container
			.row
				.col-12.col-sm-6.col-lg-4.item-prod-parent
					+item-prod-multiBlock('trimer-img.png', false, undefined, 'star-bestseller')(class="catalog-prod-item bestseller-item")
							+e.SPAN.mod-txt Бестселлер
				.col-12.col-sm-6.col-lg-4.item-prod-parent
					+item-prod-multiBlock('category-4.jpg', false)(class="catalog-prod-item")
				.col-12.col-sm-6.col-lg-4.item-prod-parent
					+item-prod-multiBlock('trimer-img.png', false, undefined, 'star-new-item')(class="catalog-prod-item new-item")
						+e.SPAN.mod-txt Новинка
				.col-12.col-sm-6.col-lg-4.item-prod-parent
					+item-prod-multiBlock('trimer-img.png', false)(class="catalog-prod-item")
				.col-12.col-sm-6.col-lg-4.item-prod-parent
					+item-prod-multiBlock('trimer-img.png', false, undefined, 'action-item' )(class="catalog-prod-item action-item")
						+e.SPAN.mod-txt Акция
				.col-12.col-sm-6.col-lg-4.item-prod-parent
					+item-prod-multiBlock('trimer-img.png', false)(class="catalog-prod-item")
