	//filters mixins
mixin FiltersBlock()
	+b.filter-bl
		+e.pop-up-controll-panel.d-flex.d-lg-none
			+e.back-btn
				+icon('angle-left')
				span Назад
			+e.discard-filters-btn
				+icon('cross')
				span сбросить фильтры
		+form-wrap()
			.check-box-filter-group
				+e.headline Фильтр
				+in-c06("checkbox", 'Самоходные', 'no-my-city')(class="filter-checkbox disabled")
				+in-c06("checkbox", 'Аккумуляторные', 'no-my-city')(class="filter-checkbox ")
				+in-c06("checkbox", 'Бензольные', 'no-my-city')(class="filter-checkbox ")
			.radio-filter-group
				+e.headline Фильтр
				+in-r('type-radio-group')(class="filter-radio disabled")
					span.check-box-cont
						span.circle
						span.txt-cont Самоходные
				+in-r('type-radio-group')(class="filter-radio")
					span.check-box-cont
						span.circle
						span.txt-cont Аккумуляторные
				+in-r('type-radio-group', true)(class="filter-radio")
					span.check-box-cont
						span.circle
						span.txt-cont Бензольные
			.price-filter-group
				+e.headline Цена
				+range("1", "999999")
			.check-box-alt-filter-group
				+e.headline Фильтр
				+in-c06("checkbox", 'Самоходные', 'no-my-city')(class="filter-checkbox-alt disabled")
				+in-c06("checkbox", 'Аккумуляторные', 'no-my-city')(class="filter-checkbox-alt ")
				+in-c06("checkbox", 'Бензольные', 'no-my-city')(class="filter-checkbox-alt ")
			.accept-filters-btn-cont.d-block.d-lg-none
				.accept-filters-btn применить фильтры

mixin range(c1, c2)
	.range-wrap.price_slider_amount
		.label-cont
			label.left-inp
				+e.text.SPAN от
				+b.INPUT.range-result--minus.minus(type="text")
			.dash-dark -
			label.right-inp
				+e.text-right.SPAN до
				+b.INPUT.range-result--plus.plus(type="text")
		input.range-slider.slider-js(type='text'  data-min= c1  data-max= c2)
		// end filters mixins

mixin Baner()
	+b.banner
		img(src="img/@2x/slider.jpg" alt='some-img')
		+e.title
			span НАСЛАЖДАЙТЕСЬ
			br
			span ИДЕАЛЬНЫМ ГАЗОНОМ
			br
			span С HUSQVARNA
			br
			span AUTOMOWER®
		a.headerBlock__btn(href='#') Узнайть больше
mixin filterBtnsBar()
	+b.filter-bar-btns.d-flex.d-lg-none
		+e.filter-btn
			+icon('filter-bar-icon')
			span ФИЛЬТР парамтеров
			.filter-number 3
		+e.cross-btn.d-none.d-sm-flex
			+icon('cross')


		//table catalog mixins
mixin catalogGoodsTableItems()
	+b.table-items-box(data-tab-for='table-box' data-tab-content='catalog-goods-display')
		+item-prod-table-sample('d-xl-none d-block' , 'category-6-png.png')
		+item-prod-table-sample('', 'category-6-png.png', 'star1.png', 'Бестселлер', '#0063da')(class="bestseller-item white-colored-item" style="background-color: #0063da")
		+item-prod-table-sample('','category-6-png.png', 'jStar.png', 'Новинка', '#18B58C')(class="new-item white-colored-item" style="background-color: #18B58C")
		+item-prod-table-sample('','category-6-png.png', 'lightning2.png','Акция', '#F35321', '#18B58C')(class="action-item white-colored-item" style="background-color: #F35321")
		+item-prod-table-sample('','category-6-png.png', 'star1.png','Акция', '#e83e8c')(class="action-item white-colored-item" style="background-color: #e83e8c")
		//#0063da

mixin item-prod-table-sample(showMoreBtnClasses='', i, imgName, spanTxt, spanBackground, btnBackground='#F35321')
	+b.table-sample-item&attributes(attributes)
		+e.header
			if imgName
				+b.img-block
					+e.IMG.background-img-bl(src=`img/@2x/${imgName}` alt="")
					+e.img-block-content
						span= spanTxt
						//(style="background:"+spanBackground+";" )
						img.lazy(src="#" data-src=`img/@2x/${i}`, alt="some-img")
			else
				+b.img-block
					+e.img-block-content
						img.lazy(src="#" data-src=`img/@2x/${i}`, alt="some-img")
			+b.char-block
				+e.headline Триммер бензиновый Husqvarna 122C
				+e.char-list.d-none.d-xl-block
					+e.char-item
						span HUS Usage
						span Occasional use
					+e.char-item
						span Рабочий объем двигателя
						span 21.7 см³
					+e.char-item
						span Рабочий объем цилиндра
						span 1.3 куб. дюйм
					+e.char-item
						span Номинальная мощность
						span 0.6 кВт
				+e.show-more-btn.toggle-chars-goods-table-js(class=showMoreBtnClasses)
					span Характеристики
					+icon('chevron-down')
			+b.buy-block
				+e.content
					+e.labels
						+e.label-link.active(type="button")
							+icon("compare")
						+e.label-link(type="button")
							+icon("like-alt")
					+e.old-price 15 990 ₽
					+e.new-price 11 490 ₽
					+e.buy-btn(style="background:"+btnBackground+";" )
						a(href="#") Купить
		+b.description-of-table-items
			+e.row-item.d-flex.d-xl-none
				span HUS Usage
				span Occasional use
			+e.row-item.d-flex.d-xl-none
				span Рабочий объем двигателя
				span 21.7 см³
			+e.row-item.d-flex.d-xl-none
				span Рабочий объем цилиндра
				span 1.3 куб. дюйм
			+e.row-item.d-flex.d-xl-none
				span Номинальная мощность
				span 0.6 кВт
			//
			+e.row-item
				span Номинальная потребляемая мощность
				span 1100 Вт
			+e.row-item
				span Напряжение сети
				span 220 - 240 В
			+e.row-item
				span Макс. производительность
				span 4000 л/ч
			+e.row-item
				span Максимальная высота всасывания
				span Максимальная высота всасывания	8 м
			+e.row-item
				span Макс. высота подачи
				span Макс. высота подачи	46 м
			+e.row-item
				span Макс. давление
				span Макс. давление	4,6 бар

